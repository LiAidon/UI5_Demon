<core:FragmentDefinition
   xmlns="sap.m"
   xmlns:core="sap.ui.core" >
   <ViewSettingsDialog confirm="onViewSettingConfirm">
		<filterItems>
			<ViewSettingsFilterItem text="Select release" 
									key="releaseKey" 
									multiSelect="true"
									filterDetailItemsAggregationChange ="filterDetailItemsAggregationChange"
									items="{path: 'devmodel>/appConfigurationSet', 
											filters:[{path:'Type', operator:'EQ',value1:'RELEASE'},{path:'Code', operator:'NE',value1:''}], 
											sorter:{ path: 'Code' , descending:'true'}}">
				<items>
					<ViewSettingsItem text="{devmodel>Value}" key="{devmodel>Code}" />
				</items>
			</ViewSettingsFilterItem>
		</filterItems>
   </ViewSettingsDialog>
</core:FragmentDefinition>