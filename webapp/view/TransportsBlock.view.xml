<mvc:View
	controllerName="ADTracker.controller.TransportsBlock"
	xmlns:l="sap.ui.layout"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m">
	<Table id="idTransports"
		inset="false"
		items="{}">
		<columns>
			<Column
				width="12em">
				<Text text="TR Number" />
			</Column>
			<Column
				minScreenWidth="Tablet"
				demandPopin="true">
				<Text text="Description" />
			</Column>
		</columns>
		<items>
			<ColumnListItem id="idTransportsItem">
				<cells>
					<ObjectIdentifier
						title="{devmodel>Tr}"/>
					<Text
						text="{devmodel>Desc}" />
				</cells>
			</ColumnListItem>
		</items>
	</Table>
</mvc:View>