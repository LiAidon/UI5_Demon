<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<ActionSheet
		title="Choose Your Action"
		showCancelButton="false"
		placement="Top">
		<buttons>
			<Button text="To Team Leader: {devmodel>teamLeadFName}" press="onSendToTeamLeader"/>
			<Button text="To Tech Leader: {devmodel>techLeadFName}" press="onSendToTechLeader"/>
			<Button text="To FA: {devmodel>faFName}" press="onSendToFA"/>
			<Button text="To Developer: {devmodel>developerFName}" press="onSendToDeveloper"/>
			<Button text="To Reviewer: {devmodel>codeReviewerFName}" press="onSendToReviewer"/>
		</buttons>
	</ActionSheet>
</core:FragmentDefinition>