<mvc:View
        xmlns:mvc="sap.ui.core.mvc"
        controllerName="ADTracker.controller.PeopleBlock"
        xmlns:l="sap.ui.layout"
        xmlns="sap.m">

    <l:Grid defaultSpan="L4 M6 S6" class="sapUiSmallMarginTop">
		<l:content>
			<ObjectListItem 
				title="{temodel>lastName} {temodel>firstName}"
				intro="{temodel>role}"
				icon= "{parts: ['temodel>userID'], formatter: '.formatter.getImageURI'}" > <!--"./images/imageID_{temodel>userID}.png" -->
				<attributes>
					<ObjectAttribute text="{temodel>telephone}" />
					<ObjectAttribute text="{temodel>mobilephone}" />
					<ObjectAttribute text="{temodel>userID}" />
					<ObjectAttribute text="{temodel>userID}@airproducts.com" />
				</attributes>
				<layoutData>
					<l:GridData span="L6 M12 S12" />
				</layoutData>
			</ObjectListItem>
			<ObjectListItem 
				title="{tlmodel>lastName} {tlmodel>firstName}"
				intro="{tlmodel>role}"
				icon= "{parts: ['tlmodel>userID'], formatter: '.formatter.getImageURI'}"> <!--"./images/imageID_{tlmodel>userID}.png"-->
				<attributes>
					<ObjectAttribute text="{tlmodel>telephone}" />
					<ObjectAttribute text="{tlmodel>mobilephone}" />
					<ObjectAttribute text="{tlmodel>userID}" />
					<ObjectAttribute text="{tlmodel>userID}@airproducts.com" />
				</attributes>
				<layoutData>
					<l:GridData span="L6 M12 S12" />
				</layoutData>
			</ObjectListItem>
			<ObjectListItem 
				title="{demodel>lastName} {demodel>firstName}"
				intro="{demodel>role}"
				icon= "{parts: ['demodel>userID'], formatter: '.formatter.getImageURI'}"> <!--"./images/imageID_{demodel>userID}.png"-->
				<attributes>
					<ObjectAttribute text="{demodel>telephone}" />
					<ObjectAttribute text="{demodel>mobilephone}" />
					<ObjectAttribute text="{demodel>userID}" />
					<ObjectAttribute text="{demodel>userID}@airproducts.com" />
				</attributes>
				<layoutData>
					<l:GridData span="L6 M12 S12" />
				</layoutData>
			</ObjectListItem>
			<ObjectListItem 
				title="{crmodel>lastName} {crmodel>firstName}"
				intro="{crmodel>role}"
				icon="{parts: ['crmodel>userID'], formatter: '.formatter.getImageURI'}"  > <!--"./images/imageID_{crmodel>userID}.png"-->
				<attributes>
					<ObjectAttribute text="{crmodel>telephone}" />
					<ObjectAttribute text="{crmodel>mobilephone}" />
					<ObjectAttribute text="{crmodel>userID}" />
					<ObjectAttribute text="{crmodel>userID}@airproducts.com" />
				</attributes>
				<layoutData>
					<l:GridData span="L6 M12 S12" />
				</layoutData>
			</ObjectListItem>
		</l:content>
	</l:Grid>
</mvc:View>