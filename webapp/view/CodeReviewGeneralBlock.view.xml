<mvc:View
        xmlns:core="sap.ui.core" 
        xmlns:mvc="sap.ui.core.mvc"
        xmlns:l="sap.ui.layout"
		xmlns:f="sap.ui.layout.form"
        class="sapUiSizeCompact"
        height="100%"
        xmlns="sap.m">
	<l:Grid
		defaultSpan="L12 M12 S12"
		width="auto">
		<l:content>
			<f:SimpleForm id="SimpleFormChange354"
				minWidth="1024"
				maxContainerCols="2"
				editable="true"
				layout="ResponsiveGridLayout"
				labelSpanL="3"
				labelSpanM="3"
				emptySpanL="4"
				emptySpanM="4"
				columnsL="2"
				columnsM="2"
				class="editableForm">
				<f:content>
					<Label text="FS Milestone Date" />
					<Input value="{devmodel>WRWKNum}" />
					<Label text="FS Receive Date" />
					<Input value="{devmodel>WRWKNum}">
					</Input>
					<Label text="FS Status" />
					<Select width="100%">
						<items>
							<core:Item text="Germany" />
							<core:Item text="USA" />
							<core:Item text="England" />
						</items>
					</Select>
					<Label text="FS Link" />
					<Input value="{devmodel>WRWKNum}">
						<layoutData>
							<l:GridData span="L2 M2 S4" />
						</layoutData>
					</Input>
					<Input value="{devmodel>WRWKNum}" />
					<Label text="FS Iteration Number" />
					<Input value="{devmodel>WRWKNum}" />
				</f:content>
			</f:SimpleForm>
		</l:content>
	</l:Grid>
</mvc:View>